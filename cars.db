import sqlite3

# Koneksi ke database
koneksi = sqlite3.connect("cars.db")
cursor = koneksi.cursor()
print("ðŸ”— Koneksi ke database berhasil.")

# Membuat tabel (jika belum ada)
cursor.execute("""
CREATE TABLE IF NOT EXISTS TBCars (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    name TEXT NOT NULL,
    brand TEXT NOT NULL,
    model TEXT NOT NULL,
    price REAL NOT NULL
)
""")
print("âœ… Tabel TBCars berhasil dibuat.")

# Menutup koneksi
koneksi.commit()
koneksi.close()
print("ðŸ”’ Koneksi ditutup dan perubahan disimpan.")
